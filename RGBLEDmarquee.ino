//--------------------------------------------------------------------------------
// GLOBALS
//--------------------------------------------------------------------------------


// Parameter 1 = number of pixels in strip
// Parameter 2 = Arduino pin number (most are valid)
// Parameter 3 = pixel type flags, add together as needed:
//   NEO_KHZ800  800 KHz bitstream (most NeoPixel products w/WS2812 LEDs)
//   NEO_KHZ400  400 KHz (classic 'v1' (not v2) FLORA pixels, WS2811 drivers)
//   NEO_GRB     Pixels are wired for GRB bitstream (most NeoPixel products)
//   NEO_RGB     Pixels are wired for RGB bitstream (v1 FLORA pixels, not v2)
Adafruit_NeoPixel strip = Adafruit_NeoPixel(STRAND_LENGTH, PIN, NEO_RGB + NEO_KHZ800);

// the message being displayed
char current_message[MAX_MESSAGE_LEN+1]="";
int current_message_length=21;
int message_width_in_pixels=0;

// the next message waiting to appear
char new_message[MAX_MESSAGE_LEN+1] = "Hello, World.";
int new_message_received=0;


//--------------------------------------------------------------------------------
// METHODS
//--------------------------------------------------------------------------------


void setup() {
  Serial.begin(BAUD);
  Serial.println(F("Hello, World."));  // F() stores the string in program memory.
  
  strip.begin();
  strip.show(); // Initialize all pixels to 'off'

  new_message_received = strlen(new_message);
  prepare_new_message();
}


void loop() {
  read_message();
  display_message();
}


// Listen for instructions from serial.
void read_message() {
  char c;
  
  while(Serial.available()) {
    c = Serial.read();
    if(new_message_received < MAX_MESSAGE_LEN) {
      new_message[new_message_received++] = c;
    }
    if(c=='\n') {
      new_message_received--;
      new_message[new_message_received] = 0;  // end string
      prepare_new_message();
      break;
    }
  }
}


// Copy the new message to the current message
void prepare_new_message() {
  Serial.print(F("Preparing '"));
  Serial.print(new_message);
  Serial.println(F("'"));
  
  current_message_length = new_message_received;
  strcpy(current_message,new_message);

  new_message_received=0;
  message_width_in_pixels = LETTER_W * current_message_length;
}


// Turn (x,y) into the correct pixel address.
// This only works because the lights are wired in an S pattern.
void p(int x,int y,long color) {
  int a = y*GRID_W;
  if( ( y % 2 ) == BACKWARDS ) {  // % is modulus.
    // y%2 is false when y is an even number - rows 0,2,4,6.
    a += x;
  } else {
    // y%2 is true when y is an odd number - rows 1,3,5,7.
    a += GRID_W - 1 - x;
  }
  a%=STRAND_LENGTH;
  strip.setPixelColor(a,color);
}


// some wavy rainbow color fun
  return strip.Color(v*(cos(        x*0.3)*127.0f+127),
                     v*(sin(        x*0.1)*127.0f+127),
                     v*(sin(3.14159+x*0.1)*127.0f+127));
}


float get_pixel(int x,int y) {
  int nx,nx2,fx,fy,letter_being_drawn;
  
  nx = x;
  nx2 = 0;
  if( nx < 0 ) {
    letter_being_drawn = 0;
  } else {
    nx = x / LETTER_W;
    if( nx > current_message_length ) {
      letter_being_drawn = 0;
    } else {
      nx2 = x % LETTER_W;
      letter_being_drawn = current_message[nx];
    }
  }
  
  fy = ( letter_being_drawn / FONT_W_CHARS ) * LETTER_H + y;
  fx = ( letter_being_drawn % FONT_W_CHARS ) * LETTER_W + nx2;
  if( fy > FONT_H_PIXELS-1 ) fy = FONT_H_PIXELS-1;
  if( fx > FONT_W_PIXELS-1 ) fx = FONT_W_PIXELS-1;
  
  // if pixel == WHITE_ON_BLACK, turn on the light
  // pgm_read_byte_near() pulls the 1 or 0 from font into RAM
  return pgm_read_byte_near(font + fy * FONT_W_PIXELS + fx);
}


void display_message() {
  // for animations
  float pixel, antialias;
  float scroll_step=-GRID_W;  // we start with the message off the right side of the grid
  int x,y;
  long clock=millis();

  do {
    if(scroll_step<0) {
      pixel = scroll_step;
      while(pixel<0) pixel++;
      antialias = pixel - (int)pixel;
    } else {
      antialias = scroll_step - (int)scroll_step;
    }

    // for every dot on the LED grid
    for(y=0;y<GRID_H;++y) {
      for(x=0;x<GRID_W;++x) {
        // find the 1 or 0 in the message to display at this dot
#if ANTIALIAS == 0        
        pixel = get_pixel(x+scroll_step,y);
#else
	    pixel = get_pixel(x+scroll_step  ,y) * (1.0f - antialias)
              + get_pixel(x+scroll_step+1,y) * (antialias);
#endif

		// draw the pixel
        p(x,
          y,
          c(x+scroll_step,y, SWIZZLE(pixel) )
        );
#if VERBOSE > 2
        Serial.print((char)letter_being_drawn);
        Serial.print(',');
        Serial.print(fy);
        Serial.print(',');
        Serial.print(fx);
        Serial.print(',');
        Serial.print( pixel,DEC);
        Serial.print('\t');
#endif
      }
#if VERBOSE > 2
      Serial.print("\n");
#endif
    }  
    strip.show();
#if VERBOSE > 1
    Serial.print(n);
    Serial.print('-');
    Serial.println(scroll_step);
#endif

    // animation time - adjust scroll_step, which will push the message to the left. 
    while( millis() - clock < FPS);
    clock=millis();
    scroll_step+= SCROLL_STEP;
	// stop after the message has gone off the grid.
  } while(scroll_step < message_width_in_pixels + GRID_W);
}


