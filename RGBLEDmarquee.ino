// IMPORTANT: To reduce NeoPixel burnout risk, add 1000 uF capacitor across
// pixel power leads, add 300 - 500 Ohm resistor on first pixel's data input
// and minimize distance between Arduino and first pixel.  Avoid connecting
// on a live circuit...if you must, connect GND first.

#include <Adafruit_NeoPixel.h>

#define VERBOSE         (0) // higher numbers for more output

#define PIN             (6)  // Arduino pin connected to LED data wire
#define STRAND_LENGTH   (64)  // How many LEDS in this strand?  An UNO can handle up to 256.

#define MAX_MESSAGE_LEN (140)  // One tweet
#define BAUD            (57600)  // Speed of serial
#define GRID_W          (8)  // Maruqee size
#define GRID_H          (8)  // Maruqee size

#define FPS             (60)  // animation smoothness
#define SCROLL_STEP     (0.3f)  // animation speed
#define BACKWARDS       (1)  // set to 0 if you wired your grid backwards
#define ANTIALIAS       (1)  // set to 0 if you don't want blending

#define SWIZZLE(x)      (x)  // white text on black background
//#define SWIZZLE(x)      (1.0-x)  // black text on white background


#define FPS_DELAY       (1000/FPS)


